{
  "name": "dndmanager",
  "version": "0.0.1",
  "description": "A small electron app to help manage some Dungeon Master tasks for 5th edition Dungeons and Dragons",
  "main": "main.js",
  "scripts": {
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "postinstall": "concurrently \"cross-env NODE_ENV=production npm run rebuild\" \"install-app-deps\"",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "dev": "NODE_ENV='development' npm run start",
    "dist": "build -mwl --x64 --ia32"
  },
  "author": {
    "name": "Dylan Sharkey",
    "email": "serghar12@gmail.com",
    "url": "https://github.com/Serghar"
  },
  "license": "MIT",
  "dependencies": {
    "font-awesome": "^4.7.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "source-map-support": "^0.4.10",
    "sqlite3": "^3.1.8",
    "webpack": "^2.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.22.0",
    "babili-webpack-plugin": "^0.0.11",
    "concurrently": "^3.1.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.26.1",
    "electron": "^1.4.12",
    "electron-builder": "^16.2.1",
    "electron-rebuild": "^1.5.7",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-compat": "^1.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "fbjs-scripts": "^0.7.1",
    "jest": "^19.0.2",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.1"
  },
  "build": {
    "productName": "Dungeon Master Manager",
    "appId": "com.sakugara.dndmanager",
    "dmg": {
      "title": "Dungeon Manager",
      "icon": "./resources/icon.icns",
      "background": "./resources/background.png"
    },
    "win": {
      "icon": "./resources/icon.ico"
    }
  },
  "keywords": [
    "electron",
    "dm",
    "dungeons and dragons",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Serghar/DnDm.git"
  },
  "homepage": "https://github.com/Serghar/DnDm#readme",
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
